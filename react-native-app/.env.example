# Android Agent AI - Environment Configuration Template
# Copy this file to .env and fill in your actual values

# ===== REQUIRED FOR APK BUILD =====
# JWT Secret for authentication (generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
JWT_SECRET=your_jwt_secret_here_minimum_32_characters

# Expo Access Token (get from: expo whoami --json)
EXPO_ACCESS_TOKEN=your_expo_access_token_here

# ===== API CONFIGURATION =====
# Backend API URL (your server)
EXPO_PUBLIC_API_URL=http://your-server.com:3000
EXPO_PUBLIC_WS_URL=ws://your-server.com:3000

# For local development
# EXPO_PUBLIC_API_URL=http://localhost:3000
# EXPO_PUBLIC_WS_URL=ws://localhost:3000

# ===== LIVEKIT STREAMING (RECOMMENDED) =====
# LiveKit Cloud (get from: https://cloud.livekit.io/)
EXPO_PUBLIC_LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# ===== PUSH NOTIFICATIONS =====
# Expo Push Token (generated after app registration)
EXPO_PUBLIC_PUSH_TOKEN=ExponentPushToken[your_push_token]

# ===== OPTIONAL ENHANCEMENTS =====
# Google Maps API Key (for enhanced mapping)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Mapbox Access Token (for premium tiles)
EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN=pk.your_mapbox_token

# ===== SECURITY =====
# Encryption key for secure storage
ENCRYPTION_KEY=your_32_character_encryption_key_here

# ===== ANALYTICS (OPTIONAL) =====
# Sentry DSN for error tracking
SENTRY_DSN=https://your_sentry_dsn@sentry.io/project_id

# Google Analytics
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX