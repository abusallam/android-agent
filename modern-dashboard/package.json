{
  "name": "modern-dashboard",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:init": "tsx src/lib/db-init.ts",
    "db:studio": "prisma studio",
    "db:setup": "npm run db:generate && npm run db:push && npm run db:init",
    "db:reset": "prisma migrate reset --force && npm run db:init",
    "db:postgres": "cp prisma/schema-postgres.prisma prisma/schema.prisma && npm run db:generate",
    "db:sqlite": "cp prisma/schema-sqlite.prisma prisma/schema.prisma && npm run db:generate",
    "test:local": "npm run db:setup && npm run dev"
  },
  "dependencies": {
    "@livekit/components-react": "^2.6.0",
    "@livekit/components-styles": "^1.1.0",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "livekit-client": "^2.8.2",
    "livekit-server-sdk": "^2.8.2",
    "lucide-react": "^0.525.0",
    "next": "15.4.3",
    "next-pwa": "^5.6.0",
    "prisma": "^6.12.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3"
  },
  "devDependencies": {
